                            .card-icon
                              i.fa.fa-shopping-cart
                              .text-right
                                h5 sales
                                h5 ugx 250,000
                          .card-footer
                            i.fa.fa-refresh
                            span update
                      // end of card
                      // card
                      .col-xl-3.col-sm-6
                        .card
